{
  "name": "@tobeno/example-booking-web-app",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "dependencies": {
    "@date-io/date-fns": "2.6.1",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "date-fns": "2.14.0",
    "qs": "6.9.4",
    "react": "16.14.0",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "use-position": "0.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "check": "npm run lint:fix; npm run type-check",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint --fix",
    "serve": "serve build --ssl-cert ../etc/ssl/server.cert --ssl-key ../etc/ssl/server.key",
    "prettier": "prettier * src/** --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "react-scripts build",
    "build:watch": "cross-env NODE_ENV=development webpack . --config node_modules/react-scripts/config/webpack.config.js -w",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "10.0.4",
    "@types/date-fns": "2.6.0",
    "@types/jest": "29.1.0",
    "@types/node": "14.14.6",
    "@types/qs": "6.9.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/testing-library__react": "10.2.0",
    "cross-env": "7.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.2.1",
    "serve": "11.3.2",
    "typescript": "4.1.2",
    "webpack-cli": "4.4.0"
  }
}
