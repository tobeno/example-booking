{
  "name": "@tobeno/example-booking-web-app",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "date-fns": "^2.8.1",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "use-position": "0.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "check": "npm run lint:fix; npm run type-check",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint --fix",
    "serve": "serve build --ssl-cert ../etc/ssl/server.cert --ssl-key ../etc/ssl/server.key",
    "prettier": "prettier * src/** --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "react-scripts build",
    "build:watch": "cross-env NODE_ENV=development webpack . --config node_modules/react-scripts/config/webpack.config.js -w",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "24.0.23",
    "@types/node": "^12.12.18",
    "@types/qs": "^6.9.0",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/testing-library__react": "^9.1.2",
    "cross-env": "^6.0.3",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest-fetch-mock": "^2.1.2",
    "prettier": "^1.19.1",
    "serve": "^11.2.0",
    "typescript": "3.5.3",
    "webpack-cli": "^3.3.10"
  }
}
