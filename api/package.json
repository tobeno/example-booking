{
  "name": "@tobeno/example-booking-api",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon --exec babel-node --extensions \".ts,.tsx\" -r node_modules/dotenv/config src/server.ts",
    "test": "node -r dotenv/config node_modules/jest/bin/jest",
    "check": "npm run lint:fix; npm run type-check",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier * etc/** public/** src/** --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "serve": "node dist/server.js",
    "ssl:regenerate": "openssl req -out etc/ssl/server.cert -newkey rsa:2048 -x509 -days 365 -nodes -keyout etc/ssl/server.key -config etc/ssl/san.cnf"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@shelf/jest-mongodb": "^1.1.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.20",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.5",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.16",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nock": "^11.3.4",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
