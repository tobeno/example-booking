{
	"name": "@tobeno/example-booking-api",
	"version": "1.0.0",
	"private": true,
	"main": "src/server.ts",
	"scripts": {
		"start": "nodemon --exec babel-node --extensions \".ts,.tsx\" -r node_modules/dotenv/config src/server.ts",
		"test": "node -r dotenv/config node_modules/jest/bin/jest",
		"check": "npm run lint:fix; npm run type-check",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "npm run lint --fix",
		"prettier": "prettier * etc/** public/** src/** --write",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build": "npm run build:types && npm run build:js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
		"serve": "node dist/server.js"
	},
	"devDependencies": {
		"@babel/cli": "7.8.4",
		"@babel/core": "7.9.6",
		"@babel/node": "7.12.10",
		"@babel/plugin-proposal-class-properties": "7.8.3",
		"@babel/plugin-proposal-numeric-separator": "7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "7.12.1",
		"@babel/preset-env": "7.9.6",
		"@babel/preset-typescript": "7.12.7",
		"@shelf/jest-mongodb": "1.2.3",
		"@types/cors": "2.8.6",
		"@types/express": "4.17.11",
		"@types/jest": "25.2.3",
		"@types/mongoose": "5.7.36",
		"@types/nock": "11.1.0",
		"@types/node": "14.14.6",
		"@types/request": "2.48.5",
		"@types/request-promise-native": "1.0.17",
		"@types/supertest": "2.0.9",
		"@typescript-eslint/eslint-plugin": "2.34.0",
		"@typescript-eslint/parser": "2.34.0",
		"dotenv": "8.2.0",
		"eslint": "7.18.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-prettier": "3.1.3",
		"jest": "26.0.1",
		"nock": "13.0.5",
		"nodemon": "2.0.6",
		"prettier": "2.2.1",
		"supertest": "4.0.2",
		"ts-jest": "26.0.0",
		"typescript": "3.9.7"
	},
	"dependencies": {
		"cors": "2.8.5",
		"express": "4.17.1",
		"mongoose": "6.4.3",
		"request": "2.88.2",
		"request-promise-native": "1.0.8"
	}
}
